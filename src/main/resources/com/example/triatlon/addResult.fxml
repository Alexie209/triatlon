<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #DCDCDC;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.triatlon.AddResultController">
   <children>
      <AnchorPane prefHeight="80.0" prefWidth="600.0" style="-fx-background-color: #1E90FF;">
         <children>
            <Label fx:id="probaLabel" layoutX="24.0" layoutY="15.0" prefHeight="51.0" prefWidth="435.0">
               <font>
                  <Font name="Comic Sans MS Bold" size="36.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <TableView fx:id="addResultTableView" layoutX="35.0" layoutY="100.0" prefHeight="275.0" prefWidth="334.0">
        <columns>
          <TableColumn fx:id="addResultFirstNameColumn" prefWidth="75.0" text="FirstName" />
          <TableColumn fx:id="addResultLastNameColumn" prefWidth="75.0" text="LastName" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <TextField fx:id="puncteObtinuteTextField" layoutX="386.0" layoutY="118.0" prefHeight="29.0" prefWidth="189.0" promptText="Puncte Obtinute" />
      <Button fx:id="addResultProbaButton" layoutX="386.0" layoutY="162.0" mnemonicParsing="false" onAction="#addResultProbaButtonOnAction" prefHeight="29.0" prefWidth="189.0" style="-fx-background-color: #FF1493;" text="Add Result">
         <font>
            <Font name="Comic Sans MS Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="addResultBackButton" layoutX="386.0" layoutY="210.0" mnemonicParsing="false" onAction="#addResultBackButtonOnAction" prefHeight="29.0" prefWidth="189.0" style="-fx-background-color: #FF1493;" text="Back">
         <font>
            <Font name="Comic Sans MS Bold" size="18.0" />
         </font>
      </Button>
      <Label fx:id="messageLabel" layoutX="384.0" layoutY="91.0" prefHeight="17.0" prefWidth="189.0" textFill="RED" />
   </children>
</AnchorPane>
